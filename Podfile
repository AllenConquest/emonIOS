# Uncomment this line to define a global platform for your project
use_frameworks!
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'

target 'emonIOS' do

pod 'InAppSettingsKit'
pod 'Alamofire'
pod 'SwiftyJSON'
pod 'Charts'

end

def testing_pods
    pod 'Quick'
    pod 'Nimble', '2.0.0-rc.3'
end

target 'emonIOSTests' do
    testing_pods
end

