# Uncomment this line to define a global platform for your project
use_frameworks!
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'

target 'emonIOS' do

pod 'InAppSettingsKit'
pod 'Alamofire'
pod 'SwiftyJSON'

end

def testing_pods
    # If you're using Xcode 6 / Swift 1.2
    pod 'Quick', '0.3.0'
    pod 'Nimble', '1.0.0'
end

target 'emonIOSTests' do
    testing_pods
end

